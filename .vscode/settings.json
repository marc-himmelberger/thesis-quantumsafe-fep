{
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]",
        "INFO"
    ],
    "todo-tree.highlights.customHighlight": {
        "INFO": {
            "icon": "log"
        },
        "BUG": {
            "icon": "bug"
        },
        "HACK": {
            "icon": "tools"
        },
        "FIXME": {
            "icon": "flame"
        },
        "XXX": {
            "icon": "comment-discussion"
        },
        "[ ]": {
            "icon": "issue-draft"
        },
        "[x]": {
            "icon": "issue-closed"
        }
    },
    "todo-tree.tree.scanMode": "workspace only",
    "go.testEnvVars": {
        "PKG_CONFIG_PATH": "$PKG_CONFIG_PATH:${workspaceFolder}/lyrebird/liboqs-go/.config-static",
    },
    "git.ignoredRepositories": [
        "${workspaceFolder}/lyrebird/liboqs",
        "${workspaceFolder}/lyrebird/liboqs-go"
    ],
}